<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.labmate.warmach.labmate">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LandingActivity"/>
        <activity android:name=".ResistorActivity"/>
        <activity android:name=".CapacitorActivity"/>
        <activity android:name=".InductorActivity"/>
        <activity android:name=".DigitalComponentsActivity"/>
        <activity android:name=".GateInformationActivity"/>
        <activity android:name=".AnalogComponentsActivity"/>
        <activity android:name=".OpampConfigurationActivity"/>
        <activity android:name=".Timer555Activity"/>
        <activity android:name=".Decoder38Activity"/>
    </application>

</manifest>