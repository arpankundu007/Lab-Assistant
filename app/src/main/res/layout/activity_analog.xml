<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="8dp"
    android:background="@drawable/loginbackground">
    <RelativeLayout android:layout_height="wrap_content"
        android:layout_width="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/opamp_relativeLayout"
        android:background="@drawable/border_thin"
        android:layout_below="@+id/textView39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:paddingRight="5dp">

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:id="@+id/imageButton14"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:adjustViewBounds="true"
            android:background="@drawable/analog"
            android:src="@drawable/border_thin" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="741 Operational Amplifier"
            android:id="@+id/textView38"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageButton14"
            android:layout_toEndOf="@+id/imageButton14"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:textSize="20dp"
            android:textColor="#666666" />
    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Select an analog component"
        android:id="@+id/textView39"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#666666" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout2"
        android:background="@drawable/border_thin"
        android:layout_below="@+id/opamp_relativeLayout"
        android:layout_alignLeft="@+id/opamp_relativeLayout"
        android:layout_alignStart="@+id/opamp_relativeLayout"
        android:layout_marginTop="10dp">

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:id="@+id/imageButton15"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:adjustViewBounds="true"
            android:background="@drawable/analog"
            android:src="@drawable/border_thin" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Common Base Configuration"
            android:id="@+id/textView40"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageButton15"
            android:layout_toEndOf="@+id/imageButton15"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:textSize="20dp"
            android:textColor="#666666" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout3"
        android:background="@drawable/border_thin"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignLeft="@+id/relativeLayout2"
        android:layout_alignStart="@+id/relativeLayout2"
        android:layout_marginTop="10dp">

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:id="@+id/imageButton16"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:adjustViewBounds="true"
            android:background="@drawable/analog"
            android:src="@drawable/border_thin" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Common Collector Configuration"
            android:id="@+id/textView41"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageButton16"
            android:layout_toEndOf="@+id/imageButton16"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:textSize="20dp"
            android:textColor="#666666" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout4"
        android:background="@drawable/border_thin"
        android:layout_below="@+id/relativeLayout3"
        android:layout_alignLeft="@+id/relativeLayout3"
        android:layout_alignStart="@+id/relativeLayout3"
        android:layout_marginTop="10dp">

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:id="@+id/imageButton17"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:adjustViewBounds="true"
            android:background="@drawable/analog"
            android:src="@drawable/border_thin" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Common Emitter Configuration"
            android:id="@+id/textView42"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageButton17"
            android:layout_toEndOf="@+id/imageButton17"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:textSize="20dp"
            android:textColor="#666666" />
    </RelativeLayout>

        </RelativeLayout>
</ScrollView>